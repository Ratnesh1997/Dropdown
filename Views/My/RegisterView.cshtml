


@model Dropdown.Models.Registration

@{
    ViewData["Title"] = "RegistrationDetails";
}

@Html.Raw(ViewBag.message)

<form asp-action="RegistrationDetails">
    <div class="form-group">
        <label for="exampleInputEmail1">First Name</label>
        <input type="text" asp-for="FirstName" placeholder="Enter First Name">

    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Last Name</label>
        <input type="text" asp-for="LastName" placeholder="Enter Last Name">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Gender</label>
        <input type="text" asp-for="Gender" placeholder="Enter Gender">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Email</label>
        <input type="text" asp-for="Email" placeholder="Enter Email">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Password </label>
        <input type="text" asp-for="Password" placeholder="Enter Password">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Confirm Password</label>
        <input type="text" asp-for="ConfirmPassword" placeholder="Confirm Password">
    </div>

    @*<div class="form-group">
            <label for="exampleInputPassword1">Mobile No</label>
            <select type="text" asp-for="@Model.Dropdown.Country" placeholder="Enter Name">Select</select>
        </div>*@


    @*<div class="form-group">
            <label for="exampleInputPassword1">Address</label>
            <input type="text" asp-for="Address" placeholder="Enter Name">
        </div>*@
    <div class="form-group">
        <button type="submit" class="btn btn-primary">Sign Up</button>
    </div>

</form>

